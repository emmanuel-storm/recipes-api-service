datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Recipe {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  ingredient String[]
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  password String
}
